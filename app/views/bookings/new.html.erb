<h1>Create a new booking for surfboard</h1>
<h1>View Your Bookings 🌊 🏄🏽‍♂️</h1>
<p>Surf in style 🏄🏾</p>
<!-- app/views/restaurants/_form.html.erb -->
<%= simple_form_for [@surfboard, @booking] do |f| %>
  <%= f.label :start_time, "Start Time" %>
  <%= f.datetime_select :start_time %>
  <%= f.label :end_time, "End Time" %>
  <%= f.datetime_select :end_time %>

  <%= f.submit "Create Booking" %>
<% end %>
